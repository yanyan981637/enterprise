<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magezon_Core::extensions" title="Magezon">
                    <resource id="Magezon_ProductLabels::label" title="Product Labels" translate="title" sortOrder="10">
                        <resource id="Magezon_ProductLabels::label_save" title="Save Label" translate="title" sortOrder="10" />
                        <resource id="Magezon_ProductLabels::label_delete" title="Delete Label" translate="title" sortOrder="20" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magezon_ProductLabels::settings" title="Product Labels" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>